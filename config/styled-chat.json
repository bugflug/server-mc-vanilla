{
  "CONFIG_VERSION_DONT_TOUCH_THIS": 2,
  "_comment": "Before changing anything, see https://github.com/Patbox/StyledChat#configuration",
  "defaultStyle": {
    "displayName": "${default}",
    "chat": "${player}: ${message}",
    "join": "<yellow><lang:multiplayer.player.joined:'${player}'></yellow>",
    "joinRenamed": "<yellow><lang:multiplayer.player.joined.renamed:'${player}' ('${old_name}')></yellow>",
    "joinFirstTime": "<yellow><lang:multiplayer.player.joined:'${player}'> (For the first time!)</yellow>",
    "left": "<yellow><lang:multiplayer.player.left:'${player}'></yellow>",
    "death": "<red>${default_message}</red>",
    "advancementTask": "<lang:chat.type.advancement.task:'${player}':'${advancement}'>",
    "advancementChallenge": "<lang:chat.type.advancement.challenge:'${player}':'${advancement}'>",
    "advancementGoal": "<lang:chat.type.advancement.goal:'${player}':'${advancement}'>",
    "teamChatSent": "<lang:'chat.type.team.sent':'<hover\\:\\'<lang\\:chat.type.team.hover>\\'><suggest_command\\:\\'/teammsg \\'>${team}':'${displayName}':'${message}'>",
    "teamChatReceived": "<lang:'chat.type.team.text':'<hover\\:\\'<lang\\:chat.type.team.hover>\\'><suggest_command\\:\\'/teammsg \\'>${team}':'${displayName}':'${message}'>",
    "privateMessageSent": "<grey>${sender}: ${message}</grey> <dark_grey>(to ${receiver})</dark_grey>",
    "privateMessageReceived": "<grey>${sender}: ${message}</grey> <dark_grey>(to ${receiver})</dark_grey>",
    "sayCommand": "${player}: ${message} <dark_grey>*</dark_grey>",
    "meCommand": "<yellow>${player} ${message}</yellow>"
  },
  "permissionStyles": [],
  "petDeathMessage": "${default_message}",
  "emoticons": {
    "item": "[%player:equipment_slot/mainhand%]",
    "shrug": "¯\\_(ツ)_/¯",
    "pos": "%player:pos_x% %player:pos_y% %player:pos_z%",
    "table": "(╯°□°）╯︵ ┻━┻"
  },
  "permissionEmoticons": [],
  "legacyChatFormatting": true,
  "parseLinksInChat": true,
  "enableMarkdown": true,
  "allowModdedDecorators": true,
  "sendFullMessageInChatPreview": true,
  "linkStyle": "<hover:show_text:'link to <blue>${link}</blue>'><underline>${link}</underline></hover>",
  "spoilerStyle": "${spoiler}",
  "spoilerSymbol": "▌",
  "defaultEnabledFormatting": {
    "dark_red": true,
    "underline": true,
    "open_url": false,
    "italic": true,
    "dark_blue": true,
    "red": true,
    "aqua": true,
    "score": false,
    "run_command": false,
    "white": true,
    "dark_gray": true,
    "spoiler": true,
    "obfuscated": false,
    "dark_grey": false,
    "suggest_command": false,
    "raw_style": false,
    "item": true,
    "em": false,
    "bold": true,
    "click": false,
    "rb": false,
    "rainbow": false,
    "reset": false,
    "page": false,
    "copy_to_clipboard": false,
    "color": false,
    "yellow": true,
    "insert": false,
    "hard_gradient": false,
    "dark_purple": true,
    "gold": true,
    "hover": false,
    "gray": true,
    "light_purple": true,
    "pos": true,
    "strikethrough": true,
    "lang": false,
    "key": false,
    "change_page": false,
    "st": true,
    "b": true,
    "green": true,
    "c": false,
    "dark_green": true,
    "gradient": false,
    "black": true,
    "i": true,
    "gr": false,
    "grey": false,
    "orange": false,
    "obf": false,
    "colour": false,
    "blue": true,
    "dark_aqua": true,
    "underlined": false,
    "keybind": false,
    "clear_color": false,
    "font": false
  }
}